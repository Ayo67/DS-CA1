{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "Airline": {
            "additionalProperties": false,
            "properties": {
                "country": {
                    "type": "string"
                },
                "established": {
                    "type": "string"
                },
                "head_quarters": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "logo": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "popularity": {
                    "type": "number"
                },
                "slogan": {
                    "type": "string"
                },
                "website": {
                    "type": "string"
                }
            },
            "required": [
                "country",
                "established",
                "head_quarters",
                "id",
                "logo",
                "name",
                "popularity",
                "slogan",
                "website"
            ],
            "type": "object"
        },
        "AirlineFleet": {
            "additionalProperties": false,
            "properties": {
                "aircraftName": {
                    "type": "string"
                },
                "airlineId": {
                    "type": "number"
                },
                "capacity": {
                    "type": "number"
                },
                "range": {
                    "type": "string"
                }
            },
            "required": [
                "aircraftName",
                "airlineId",
                "capacity",
                "range"
            ],
            "type": "object"
        }
    }
}

